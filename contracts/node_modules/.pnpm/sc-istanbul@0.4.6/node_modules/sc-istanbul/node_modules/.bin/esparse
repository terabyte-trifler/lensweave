#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules/esprima/bin/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules/esprima/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules/esprima/bin/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules/esprima/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/esprima@2.7.3/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@2.7.3/node_modules/esprima/bin/esparse.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@2.7.3/node_modules/esprima/bin/esparse.js" "$@"
fi
