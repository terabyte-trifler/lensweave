#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/plugins/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/plugins/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules/solidity-coverage/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/solidity-coverage@0.8.16_hardhat@2.26.3_ts-node@10.9.2_@types+node@24.2.1_typescript@5.9.2__typescript@5.9.2_/node_modules:/Users/terabyte_trifler/Documents/lensWeave/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../solidity-coverage/plugins/bin.js" "$@"
fi
